{
  "name": "<%= optAppName %>",
  "version": "0.0.0",
  "homepage": "https://github.com/<%= optGithubName%>/<%= optAppName %>",
  "authors": [
    "<%= optAuthorName %>"
  ],
  "description": "<%= optAppDescription %>",
  "keywords": [
    <% for(var ii=0; ii<optAppKeywords.length; ii++) {
      if(ii !==0) {
        print('    ');
      }
      print('"'+optAppKeywords[ii]+'"');
      if(ii !==(optAppKeywords.length -1)) {
        print(',\n');
      }
    }
    %>
  ],
  "dependencies": {
    "angular": "1.4.7",
    "angular-animate": "1.4.7",
    "angular-sanitize": "1.4.7",
    "angular-touch": "1.4.7",
    "angular-route": "1.4.7",
    "angular-cookies": "1.4.7",
	"angular-mocks": "1.4.7",
    "lesshat": "latest",
    "font-awesome": "latest",
    "less-flexbox": "latest",
    "momentjs": "latest",
    "angular-string": "latest",
    "angular-array": "latest",
    "angular-forminput": "latest",
    "angular-socket-io": "latest"
  },
  "license": "MIT",
  "resolutions": {
    "angular": "1.4.7",
    "angular-animate": "1.4.7",
    "angular-sanitize": "1.4.7",
    "angular-touch": "1.4.7"
  }
}
